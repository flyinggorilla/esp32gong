{
    "configurations": [
        {
            "name": "ESP-IDF",
            "includePath": [
                "${default}",
                "${env:IDF_PATH}/components/**",
                "${workspaceFolder}/**"
            ],
            "defines": [
                "_DEBUG",
                "UNICODE",
                "_UNICODE"
            ],
            "cStandard": "c17",
            "cppStandard": "c++17",
            "compilerPath": "xtensa-esp32-elf-gcc.exe",
            "compilerArgs": [
                "${env:IDF_PATH}/**"
            ],
            "compileCommands": "${workspaceFolder}/build/compile_commands.json"
        }
    ],
    "version": 4
}